            <Condition>(proxy.pathsuffix MatchesPath "/offices/active") and (request.verb = "GET")</Condition>
        </Flow>
        <Flow name="getofficebyid">
            <Description>GetOfficeById</Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/offices/{id}") and (request.verb = "GET")</Condition>
        </Flow>
        <Flow name="getteamdelta">
            <Description>GetTeamDelta</Description>
            <Request/>
