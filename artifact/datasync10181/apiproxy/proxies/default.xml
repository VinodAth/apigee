            <Condition>(proxy.pathsuffix MatchesPath "/teams/delta") and (request.verb = "GET")</Condition>
        </Flow>
        <Flow name="getactiveteams">
            <Description>GetActiveTeams</Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/teams/active") and (request.verb = "GET")</Condition>
        </Flow>
        <Flow name="getteambyid">
            <Description>GetTeamById</Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/teams/{id}") and (request.verb = "GET")</Condition>
        </Flow>
    </Flows>
    <PostFlow name="PostFlow">
        <Request/>
        <Response>
            <Step>
                <FaultRules/>
                <Name>FC-MessageLogger</Name>
            </Step>
        </Response>
    </PostFlow>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <FaultRules/>
                <Name>FC-ExtractParameters</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>FC-CommonConfig</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>FC-SpikeArrest</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>FC-APIKey</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>FC-JWT</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <HTTPProxyConnection>
        <BasePath>/v1.0/datasync1018</BasePath>
        <Properties/>
        <VirtualHost>default</VirtualHost>
        <VirtualHost>secure</VirtualHost>
    </HTTPProxyConnection>
    <RouteRule name="default">
        <TargetEndpoint>default</TargetEndpoint>
    </RouteRule>
</ProxyEndpoint>
