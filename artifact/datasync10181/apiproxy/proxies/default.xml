        <Flow name="getcompanybyid">
            <Description>GetCompanyById</Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/companies/{id}") and (request.verb = "GET")</Condition>
        </Flow>
        <Flow name="getlistingdelta">
            <Description>GetListingDelta</Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/listings/delta") and (request.verb = "GET")</Condition>
        </Flow>
        <Flow name="getactivelistings">
            <Description>GetActiveListings</Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/listings/active") and (request.verb = "GET")</Condition>
        </Flow>
        <Flow name="getlistingbyid">
            <Description>GetListingById</Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/listings/{id}") and (request.verb = "GET")</Condition>
        </Flow>
        <Flow name="getofficedelta">
            <Description>GetOfficeDelta</Description>
            <Request/>
