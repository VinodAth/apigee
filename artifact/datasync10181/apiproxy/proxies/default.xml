<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
    <Description>DataSync-26-09</Description>
    <FaultRules/>
    <Flows>
        <Flow name="getagentdelta">
            <Description>GetAgentDelta</Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/agents/delta") and (request.verb = "GET")</Condition>
        </Flow>
        <Flow name="getactiveagents">
            <Description>GetActiveAgents</Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/agents/active") and (request.verb = "GET")</Condition>
        </Flow>
        <Flow name="getagentbyid">
            <Description>GetAgentById</Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/agents/{id}") and (request.verb = "GET")</Condition>
        </Flow>
