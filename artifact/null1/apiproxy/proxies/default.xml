<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
    <Description>Search (UnSecured)</Description>
    <FaultRules/>
    <Flows>
        <Flow name="GetListings">
            <Description></Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/listings") and (request.verb = "GET")</Condition>
        </Flow>
        <Flow name="GetListingsPost">
            <Description></Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/listings") and (request.verb = "POST")</Condition>
        </Flow>
        <Flow name="GetListingById">
            <Description></Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/listings/{listingId}") and (request.verb = "GET")</Condition>
        </Flow>
        <Flow name="GetListingsDynamic">
            <Description></Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/listings/dynamic") and (request.verb = "POST")</Condition>
        </Flow>
        <Flow name="SuggestListingsAsync">
            <Description></Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/listings/suggest") and (request.verb = "GET")</Condition>
        </Flow>
        <Flow name="SuggestListingsPostAsync">
            <Description></Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/listings/suggest") and (request.verb = "POST")</Condition>
        </Flow>
        <Flow name="GetOffices">
            <Description></Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/offices") and (request.verb = "GET")</Condition>
        </Flow>
        <Flow name="GetOfficeById">
            <Description></Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/offices/{officeId}") and (request.verb = "GET")</Condition>
        </Flow>
        <Flow name="GetAgents">
            <Description></Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/agents") and (request.verb = "GET")</Condition>
        </Flow>
        <Flow name="GetAgentById">
            <Description></Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/agents/{agentId}") and (request.verb = "GET")</Condition>
        </Flow>
        <Flow name="GetCompanies">
            <Description></Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/companies") and (request.verb = "GET")</Condition>
        </Flow>
        <Flow name="GetCompanyById">
            <Description></Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/companies/{companyId}") and (request.verb = "GET")</Condition>
        </Flow>
        <Flow name="GetTeams">
            <Description></Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/teams") and (request.verb = "GET")</Condition>
        </Flow>
        <Flow name="GetTeamById">
            <Description></Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/teams/{teamId}") and (request.verb = "GET")</Condition>
        </Flow>
        <Flow name="GetLocations">
            <Description></Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/geographies") and (request.verb = "GET")</Condition>
        </Flow>
    </Flows>
    <PostFlow name="PostFlow">
        <Request/>
        <Response>
            <Step>
                <FaultRules/>
                <Name>FC-MessageLogger</Name>
            </Step>
        </Response>
    </PostFlow>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <FaultRules/>
                <Name>FC-SpikeArrest</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>FC-APIKey</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>FC-JWT</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <HTTPProxyConnection>
        <BasePath>/v1.0.0/null</BasePath>
        <Properties/>
        <VirtualHost>default</VirtualHost>
        <VirtualHost>secure</VirtualHost>
    </HTTPProxyConnection>
    <RouteRule name="default">
        <TargetEndpoint>default</TargetEndpoint>
    </RouteRule>
</ProxyEndpoint>
